package firstcode;

public class DataTypes {

	public static void main(String[] args) {
		
		int cd= 100; // int type data 
		double vd=15.2;
		char cf = 'a';
		
		boolean bd =  true;
		
		String s = "Welcome";
		
		System.out.println(s);
		
		
		//OPERATORS**************************88
		
		int a = 10;
		int b = 20;
		System.out.println("sum of:"+ (a+b));
		
		//Relational operator************
		// > >= < <= != ==
		System.out.println(a<b);
		System.out.println(a<=b);
		System.out.println(a>=b);
		
		
		
		

	}

}
